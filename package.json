{
  "name": "extract-gsheet",
  "version": "0.1.6",
  "description": "Quickly parse a public Google Sheet into JSON for use on any website.",
  "main": "./src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngblaylock/extract-gsheet"
  },
  "homepage": "https://ngblaylock.github.io/extract-gsheet/",
  "scripts": {
    "dev": "npm run clean:dev && cross-env ELEVENTY_ENV=dev npx @11ty/eleventy --serve --quiet",
    "build": "npm run move-readme && npm run clean && cross-env ELEVENTY_ENV=prod npx @11ty/eleventy",
    "clean:dev": "npx rimraf ./dev",
    "clean": "npx rimraf ./docs ./dev",
    "move-readme": "node move-readme",
    "rollup": "rollup -c",
    "rollup:watch": "rollup -c -w"
  },
  "keywords": [
    "Google",
    "Sheets",
    "Spreadsheets",
    "Prototype"
  ],
  "author": "Nathan Blaylock",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "bootstrap": "^4.6.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "eleventy-plugin-sass": "^1.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
